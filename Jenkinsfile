pipeline {
    
    agent any 
            stages{
                stage('init'){
                    steps{
                        echo('hello')
                    }
                }
                stage('build'){
                    steps{
                        echo('hello build stage')
                    }
                }

                stage('deploy'){
                    steps{
                        echo('Hello deployement')
                    }
                } 
            }

    }