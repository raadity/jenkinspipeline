pipeline {
    
    agent any 
            stages{
                stage('init'){
                    steps{
                        echo("hello ")
                    }
                }
                stage("build"){
                    step{
                        echo("hello build stage ")
                    }
                }

                stage("deploy"){
                    step{
                        echo("Hello deployement ")
                    }
                } 
            }

    }